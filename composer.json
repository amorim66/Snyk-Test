{
    "name": "seu-projeto/loja-virtual",
    "description": "Sistema de loja virtual com integrações de marketplaces",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "twig/twig": "^3.4.3",
        "guzzlehttp/guzzle": "^7.4.4",
        "guzzlehttp/psr7": "^2.4.5",
        "symfony/http-foundation": "^5.4",
        "symfony/security-csrf": "^5.4",
        "symfony/validator": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
} 